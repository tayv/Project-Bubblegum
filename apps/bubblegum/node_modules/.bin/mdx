#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/bin/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/bin/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/mdx@0.3.1/node_modules:/Users/taylor/Documents/GitHub/project_bubblegum/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/bin/mdx" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/mdx@0.3.1/node_modules/mdx/bin/mdx" "$@"
fi
